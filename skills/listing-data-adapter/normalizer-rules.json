  "incentivePatterns": [
    {
      "regex": "free\\s*month",
      "flags": "i",
      "label": "free month"
    },
    {
      "regex": "\\d+\\s*months?\\s*free",
      "flags": "i",
      "label": "X months free"
    },
    {
      "regex": "no\\s*(broker\\s*)?fee",
      "flags": "i",
      "label": "no fee"
    },
    {
      "regex": "owner\\s*pays(\\s*fee)?",
      "flags": "i",
      "label": "owner pays fee"
    },
    {
      "regex": "concession",
      "flags": "i",
      "label": "concession"
    },
    {
      "regex": "reduced\\s*deposit",
      "flags": "i",
      "label": "reduced deposit"
    },
    {
      "regex": "flexible\\s*lease",
      "flags": "i",
      "label": "flexible lease"
    },
    {
      "regex": "move.?in\\s*special",
      "flags": "i",
      "label": "move-in special"
    },
    {
      "regex": "rent\\s*stabilized",
      "flags": "i",
      "label": "rent stabilized"
    },
    {
      "regex": "net\\s*effective",
      "flags": "i",
      "label": "net effective rent"
    },
    {
      "regex": "first\\s*(month|mo)\\s*free",
      "flags": "i",
      "label": "first month free"
    },
    {
      "regex": "last\\s*(month|mo)\\s*free",
      "flags": "i",
      "label": "last month free"
    }
  ],

